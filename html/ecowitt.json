{
  "name": "Ecowitt HTML template repository",
  "templates": {
    "2": "<div><i class=\"ewi-air\" style=\"color:#${aqiColor}\"></i> ${aqiDanger}<br>PM2.5: ${pm25} &micro;g/m&sup3;<br>AQI: ${aqi}</div>"
  },
  "templatesImperial": {
    "0": "<div><i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %</div>",
    "1": "<div><i class=\"ewi-temperature\"></i> ${temperature} &deg;F<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} inHg</div>",
    "3": "<div><i class=\"ewi-windspeed\"></i> ${windSpeed} mhp<br><i class=\"ewi-wind${windCompass}\"></i> ${windDirection}&deg; ${windCompass}<br><i class=\"ewi-rain\"></i> ${rainRate} in/h<br><i class=\"ewi-light\"></i> ${illuminance} lux<br>UV ${ultravioletDanger}</div>"
  },
  "templatesMetric": {
    "0": "<div><i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %</div>",
    "1": "<div><i class=\"ewi-temperature\"></i> ${temperature} &deg;C<br><i class=\"ewi-humidity\"></i> ${humidity} %<br><i class=\"ewi-pressure\"></i> ${pressure} hPa</div>",
    "3": "<div><i class=\"ewi-windspeed\"></i> ${windSpeed} km/h<br><i class=\"ewi-wind${windCompass}\"></i> ${windDirection}&deg; ${windCompass}<br><i class=\"ewi-rain\"></i> ${rainRate} mm/h<br><i class=\"ewi-light\"></i> ${illuminance} lux<br>UV ${ultravioletDanger}</div>"
  }
}
